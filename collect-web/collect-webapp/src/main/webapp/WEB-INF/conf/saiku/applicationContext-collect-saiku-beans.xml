<?xml version="1.0" encoding="UTF-8" ?>

<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.1.xsd">

	<bean id="sessionService" class="org.openforis.collect.saiku.SaikuSessionService" />

	<bean id="saikuMondrianSchemaProvider" class="org.openforis.collect.saiku.SaikuMondrianSchemaProvider"
			lazy-init="false" init-method="init">
		<constructor-arg ref="surveyManager" />
	</bean>
	
	<bean id="repositoryDsManager" class="org.openforis.collect.saiku.CollectSaikuDatasourceManager" 
		init-method="load" destroy-method="unload"/>
	
</beans>
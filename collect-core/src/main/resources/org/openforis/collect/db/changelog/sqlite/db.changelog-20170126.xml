<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd">
 
  	<changeSet id="add-survey-institution-id" author="ricci" dbms="sqlite">
		<comment>Add survey institution id column</comment>
		<sql>
			alter table ofc_survey add column institution_id bigint;			
		</sql>
	</changeSet>
	
	<changeSet id="add-code-list-item-color-column" author="ricci" dbms="sqlite">
		<sql>
			alter table ofc_code_list add column color char(6);			
		</sql>
	</changeSet>
	
</databaseChangeLog>
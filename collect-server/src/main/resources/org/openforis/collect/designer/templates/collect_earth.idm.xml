	<spatialReferenceSystems>
		<spatialReferenceSystem srid="EPSG:4326">
			<label xml:lang="en">EPSG:4326</label>
			<description xml:lang="en">EPSG:World Geodetic System 1984
				Horizontal component of 3D system. Used by the GPS satellite
				navigation system and for NATO military geodetic surveying.
			</description>
			<wkt><![CDATA[
	        GEOGCS["WGS 84", 
			  DATUM["World Geodetic System 1984", 
			    SPHEROID["WGS 84", 6378137.0, 298.257223563, AUTHORITY["EPSG","7030"]], 
			    AUTHORITY["EPSG","6326"]], 
			  PRIMEM["Greenwich", 0.0, AUTHORITY["EPSG","8901"]], 
			  UNIT["degree", 0.017453292519943295], 
			  AXIS["Geodetic longitude", EAST], 
			  AXIS["Geodetic latitude", NORTH], 
			  AUTHORITY["EPSG","4326"]]
			]]></wkt>
		</spatialReferenceSystem>
	</spatialReferenceSystems>
	<schema>
		<entity id="1" name="plot" ui:tabSet="plot_tab_set"
			ui:tab="plot_tab">
			<label xml:lang="en">Plot</label>
			<label xml:lang="es">Parcela</label>
			<label xml:lang="fr">Parcelle</label>
			<text id="2" name="id" required="true" key="true">
				<label xml:lang="en">Id</label>
				<label xml:lang="es">Id</label>
				<label xml:lang="fr">Id</label>
			</text>
			<coordinate id="3" name="location">
				<label xml:lang="en">Location</label>
				<label xml:lang="es">Posicion</label>
				<label xml:lang="fr">Lieu</label>
			</coordinate>
			<text id="4" name="operator">
				<label xml:lang="en">Operator</label>
				<label xml:lang="es">Operador</label>
				<label xml:lang="fr">Opérateur</label>
			</text>

			<number id="5" name="elevation" type="integer"
				collectearth:fromcsv="true">
				<label xml:lang="en">Elevation</label>
				<label xml:lang="es">Elevacion</label>
				<label xml:lang="fr">Elevation</label>
			</number>
			<number id="6" name="slope" type="real"
				collectearth:fromcsv="true">
				<label xml:lang="en">Slope</label>
				<label xml:lang="es">Pendiente</label>
				<label xml:lang="fr">Pente</label>
			</number>
			<number id="7" name="aspect" type="real"
				collectearth:fromcsv="true">
				<label xml:lang="en">Aspect</label>
				<label xml:lang="es">Exposicion</label>
				<label xml:lang="fr">Aspect</label>
			</number>
			<boolean id="8" name="actively_saved">
				<label xml:lang="en">Actively saved by user</label>
				<label xml:lang="es">Guardado por el usuario activamente</label>
				<label xml:lang="fr">Sauvé par l'utilisateur</label>
			</boolean>
			<date id="9" name="actively_saved_on" required="false"
				relevant="false()">
				<label xml:lang="en">Last saved on</label>
				<label xml:lang="es">Guardado por ultima vez</label>
				<label xml:lang="fr">Sauvé la dernière fois sur</label>
			</date>
			<text id="10" name="plot_file" required="false"
				relevant="false()">
				<label xml:lang="en">CSV file that contains the plot</label>
				<label xml:lang="es">Archivo CSV que contiene al plot</label>
				<label xml:lang="fr">Fichier CSV qui contient le point</label>
			</text>
			<code id="11" name="calculated_elevation_range" ui:hide="true"
				ui:tab="plot_tab" calculated="true" list="elevation_ranges">
				<label type="instance">DEM - Elevation - range</label>
				<label type="instance" xml:lang="fr">MNT - Gamme d'altitude</label>
				<default
					expr=" format-number(ceiling(elevation div 100 ), '#')" />
			</code>
			<code id="12" name="calculated_aspect" ui:hide="true"
				ui:tab="plot_tab" calculated="true" list="orientation">
				<label type="instance">DEM - Aspect - ranges</label>
				<label type="instance" xml:lang="fr">MNT - Gamme d'exposition</label>
				<default value="na" if="aspect=0 and slope=0" />
				<default value="N" if="aspect&gt;337.5 or aspect&lt;=22.5" />
				<default value="NE" if="aspect&gt;22.5 and aspect&lt;=67.5" />
				<default value="E" if="aspect&gt;67.5 and aspect&lt;=112.5" />
				<default value="SE"
					if="aspect&gt;112.5 and aspect&lt;=157.5" />
				<default value="S" if="aspect&gt;157.5 and aspect&lt;=202.5" />
				<default value="SW"
					if="aspect&gt;202.5 and aspect&lt;=247.5" />
				<default value="W" if="aspect&gt;247.5 and aspect&lt;=292.5" />
				<default value="NW"
					if="aspect&gt;292.5 and aspect&lt;=337.5" />
			</code>
			<code id="13" name="calculated_slope" ui:hide="true"
				ui:tab="plot_tab" calculated="true" list="slope_classes">
				<label type="instance">DEM - Slope - ranges</label>
				<label type="instance" xml:lang="fr">MNT - Gamme de pente</label>
				<default value="FLAT" if="slope&gt;=0 and slope&lt;6" />
				<default value="SLIGHT" if="slope&gt;=6 and slope&lt;16" />
				<default value="STEEP" if="slope&gt;=16 and slope&lt;31" />
				<default value="VERY_STEEP"
					if="slope&gt;=31 and slope&lt;45" />
				<default value="EXTREME" if="slope&gt;=46 and slope&lt;61" />
				<default value="FALL" if="slope&gt;=60" />
				<default value="NA" if="idm:blank(aspect)" />
			</code>
		</entity>
	</schema>
</survey>